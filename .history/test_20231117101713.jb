defarray b 10

def i int
def max int
def tmp int
$max = 6

$i = 1

label loop1

if $i geq $max
	jmp @print
endif

scan $tmp
put $tmp &b $i
$i += 1

jmp @loop1

label print

$i = 1

label loop2

if $i geq $max
	exit
endif

get $tmp &b $i
print $tmp
sprint " "
$tmp := $i
$tmp += 1
$i := $tmp

jmp @loop2